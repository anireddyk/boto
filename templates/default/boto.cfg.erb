[Credentials]
aws_access_key_id = <%= node['boto']['aws_access_key_id'] %>
aws_secret_access_key = <%= node['boto']['aws_secret_access_key'] %>

[Boto]
debug = <%= node['boto']['debug'] %>
num_retries = <%= node['boto']['num_retries'] %>

ec2_region_name = <%= node['boto']['ec2']['region']['name'] %>
ec2_region_endpoint = <%= node['boto']['ec2']['region']['endpoint'] %>

